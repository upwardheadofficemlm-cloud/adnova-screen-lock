{
  "version": 2,
  "name": "adnova-screen-lock",
  "builds": [
    {
      "src": "website/**",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "env": {
    "FIREBASE_API_KEY": "AIzaSyAz8P_dX5uTI3zF0lW2CwPh4yqFvwfUn2o",
    "FIREBASE_AUTH_DOMAIN": "adnova-screen-lock-90521.firebaseapp.com",
    "FIREBASE_PROJECT_ID": "adnova-screen-lock-90521",
    "FIREBASE_STORAGE_BUCKET": "adnova-screen-lock-90521.firebasestorage.app",
    "FIREBASE_MESSAGING_SENDER_ID": "914767381345",
    "FIREBASE_APP_ID": "1:914767381345:android:22ee0a507344a202977b20",
    "FIREBASE_MEASUREMENT_ID": "G-J4L3HM9L45"
  }
}
